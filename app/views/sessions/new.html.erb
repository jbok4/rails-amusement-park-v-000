<h1>Sign in</h1>

<%= form_for(:session, url: signin_path) do |f| %>

  <%= f.label :name %></br>
  <%= f.text_field :name %></br></br>

  <%= f.label :password %></br>
  <%= f.password_field :password %></br></br>

  <%= f.submit "Sign in"  %></br></br>
<% end %>

<p>New user? <%= link_to "Sign up now!", new_user_path %></p>