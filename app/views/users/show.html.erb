<div class="container" role="main">
      <p id="notice">Welcome back to the theme park!</p>


  <div class="center">
    <div class="pic">
      <img src="http://s3.amazonaws.com/nvest/Blank_Club_Website_Avatar_Gray.jpg" height="250">
    </div>


    <div class="info">
      <% if @user.admin %>
        <p><strong>ADMIN</strong></p>
      <% end %>
      <br>
       <p><strong>Name: </strong><%= @user.name %></p>
       <p><strong>Tickets:</strong> <%= @user.tickets %></p>
       <p><strong>Mood:</strong> <%= @user.mood %></p>
       <p><strong>Height:</strong> <%= @user.height %></p>
    </div>
  </div>
</div>